{
  "manifest_version": 2,
  "name": "Nyaa Sorter",
  "version": "1.0",
  "description": "Sort torrents by seeders on nyaa.si",
  "permissions": [
    "*://*.nyaa.si/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.nyaa.si/*"
      ],
      "js": [
        "sortBySeeders.js",
        "highlight.js"
      ],
      "css": [
        "highlight.css"
      ]
    }
  ]
}